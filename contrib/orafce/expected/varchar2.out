(1 row)

-- returns 1 (start of the source string)
SELECT strposb('ABCありがとう', '');
 strposb 
---------
       1
(1 row)

-- returns 0
SELECT strposb('ABCありがとう', 'XX');
 strposb 
---------
       0
(1 row)

-- returns 't'
SELECT strposb('ABCありがとう', NULL) IS NULL;
 ?column? 
----------
 t
(1 row)

