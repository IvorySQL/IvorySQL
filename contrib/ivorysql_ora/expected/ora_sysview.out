	T15 TIMESTAMP WITH LOCAL TIME ZONE,
	T16 CLOB, T17 BLOB, T18 NCLOB, T19 LONG, T20 LONG RAW, T21 BOOLEAN
)
RETURN RAW IS
BEGIN
	RETURN CAST('FUNC_22_ARG_TYPES' AS RAW);
