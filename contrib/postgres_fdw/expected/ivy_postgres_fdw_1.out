-- ===================================================================
CREATE OR REPLACE FUNCTION f_test(p_c1 number(38,0)) RETURNS int AS $$
DECLARE
	v_c1 int;
BEGIN
    SELECT c1 INTO v_c1 FROM ft1 WHERE c1 = p_c1 LIMIT 1;
    PERFORM c1 FROM ft1 WHERE c1 = p_c1 AND p_c1 = v_c1 LIMIT 1;
    RETURN v_c1;
END;
$$ LANGUAGE plpgsql;
/
SELECT f_test(100);
 f_test 
--------
    100
(1 row)

