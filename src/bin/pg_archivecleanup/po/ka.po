#: pg_archivecleanup.c:165
#, c-format
msgid "could not remove file \"%s\": %m"
msgstr "ფაილის წაშლის შეცდომა \"%s\": %m"

#: pg_archivecleanup.c:170
#, c-format
msgid "could not read archive location \"%s\": %m"
