-- ROWNUM in aggregate function with ORDER BY
SELECT
    dept_id,
    MAX(ROWNUM) as max_rownum,
    MIN(ROWNUM) as min_rownum,
    COUNT(ROWNUM) as count_rownum
FROM (
    SELECT dept_id, ROWNUM
    FROM rownum_test
    ORDER BY value DESC
