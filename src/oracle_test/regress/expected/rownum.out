--
-- Edge cases for ROWNUM reset
--
-- ROWNUM in EXISTS correlated subquery
SELECT
    id,
    EXISTS(SELECT 1 FROM rownum_test t2 WHERE t2.id = t1.id AND ROWNUM = 1) as has_first
FROM rownum_test t1
WHERE id <= 5
ORDER BY id;
 id | has_first 
----+-----------
  1 | t
  2 | t
  3 | t
  4 | t
  5 | t
(5 rows)

