CREATE FUNCTION test_oat_schema.test_trigger()
RETURNS trigger
LANGUAGE plpgsql
AS $$
BEGIN
  IF TG_OP = 'DELETE'
  THEN
    RETURN OLD;
  ELSE
    RETURN NEW;
  END IF;
END; $$;
